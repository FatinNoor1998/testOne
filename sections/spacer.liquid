<Section class="section-wrapper-{{ section.id }} width-{{ section.settings.section-width }}">
  {% for block in section.blocks %}
    {% case block.type %}
        {% when 'spacer-block' %}
            <div class="spacer-{{ block.id }}">
                
            </div>
            
    {% endcase %}
  {% endfor %}
</Section>

<style>
  {% for block in section.blocks %}
    {% case block.type %}
    {% when 'spacer-block' %}
    .section-wrapper-{{ section.id }} .spacer-{{ block.id }}{
      height: {{ block.settings.spacer-height }}px;
    }

    {% endcase %}
    {% endfor %}
</style>

{% schema %}
  {
    "name": "Section name",
    "blocks": [
      {
        "type": "spacer-block",
        "name": "Spacer",
        "settings": [
          {
            "type": "range",
            "id": "spacer-height",
            "label": "Spacer Height",
            "min": 0,
            "max": 200,
            "step": 5,
            "unit": "px",
            "default": 10
          }
        ]
      }
    ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

